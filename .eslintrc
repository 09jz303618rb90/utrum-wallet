{
  "extends": "@monaize/eslint-config-monaize",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "rules": {
    "import/no-unresolved": "off",
    "no-shadow": "off",
    "no-param-reassign": "off",
    "no-bitwise": "off"
  },
  "globals": {
    "$": true,
    "jQuery": true
  }
}